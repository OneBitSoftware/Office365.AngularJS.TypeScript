{
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "authors": [ "Radi Atanassov, OneBit Software" ],
  "description": "A demo showing how to work with Office 365 data from a SPA build with AngularJS and TypeScript.",
  "compilationOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-final"
  },

  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel"
  },

  "frameworks": {
    "dnx451": { "dependencies": {} },
    "dnxcore50": { }
  },

  "exclude": [
    "wwwroot",
    "node_modules"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc"
  ]
}
